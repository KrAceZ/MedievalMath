<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<AnchorPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.medievalmath.ProfilePageController">
    <!-- Buttons corresponding to the text locations in the image -->
    <ImageView fx:id="ProfilePageBackgroundView" fitWidth="600" preserveRatio="true" />

    <AnchorPane fx:id="profilePageContainer" prefHeight="380.0" prefWidth="600.0">
        <Button fx:id="levelsButton" layoutX="100" layoutY="50" text="Levels" />
        <Button fx:id="workScrollsButton" layoutX="200" layoutY="50" text="Work: Scrolls" />
        <Button fx:id="questsButton" layoutX="300" layoutY="50" text="Quests" />
        <Button fx:id="trophiesButton" layoutX="400" layoutY="50" text="Trophies" />
        <Button fx:id="shopButton" layoutX="500" layoutY="50" text="Shop" />
        <Button fx:id="homeButton" layoutX="600" layoutY="50" text="Home" />
        <Button fx:id="logOutButton" layoutX="700" layoutY="50" text="Log Out" />
    </AnchorPane>

</AnchorPane>
